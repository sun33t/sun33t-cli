{
	"compilerOptions": {
		"allowJs": true,
		"target": "ESNext",
		"module": "ES6",
		"esModuleInterop": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"lib": ["DOM", "ESNext"],
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"outDir": "dist"
	}
}
